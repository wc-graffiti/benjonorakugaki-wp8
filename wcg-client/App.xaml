<Application
    x:Class="wcg_client.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:wcg_client">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ApiParameters.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <x:Double x:Key="defaultAccuracy">50.0</x:Double>
        </ResourceDictionary>
    </Application.Resources>

</Application>
    
